#include<iostream>
using namespace std;
int main()
{
    int pocketMoney = 9000;
    for (int date = 0; date <=30; date++)
    {
        if(date%2==0)
        {
           continue;
        }
        cout<<"Go outside Guys "<<endl;
        pocketMoney -= 300;
        if(pocketMoney ==0)
        {
            break;
        }
    }
    
    
}
