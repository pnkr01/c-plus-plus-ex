WAP to find prime number between two number ?


#include<iostream>
using namespace std;
int main()
{
    int i,j,a,b;
    cout<<"Enter first number..";
    cin>>a;
    cout<<"Enter Second number..";
    cin>>b;
    for (i = a; i <= b; i++)
    {
       for (j = 2; j < i; j++)
       {
           if(i%j==0)
           {
               break;
           }
       }
        if (j==i)
            {
               cout<<i;
           }
                 
    }
    return 0;
}
